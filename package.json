{
  "name": "@sleeksky/translate",
  "version": "1.0.0",
  "description": "Makes managing internationalization easier.",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "test": "npm run cover",
    "test:prod": "cross-env BABEL_ENV=production npm run test",
    "test:only": "mocha --require babel-core/register --require babel-polyfill --recursive",
    "test:watch": "npm test -- --watch",
    "test:examples": "node examples/",
    "cover": "nyc npm run test:only",
    "lint": "eslint src test",
    "build": "cross-env BABEL_ENV=production babel src --out-dir lib",
    "prepublish": "npm run clean && npm run test && npm run build",
    "types": "cross-env BABEL_ENV=production babel src/types.js --out-dir lib"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sleeksky-dev/translate.git"
  },
  "keywords": [
    "i18n",
    "internationalization",
    "translation",
    "language"
  ],
  "author": "Yusuf Bhabhrawala",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sleeksky-dev/translate/issues"
  },
  "homepage": "https://github.com/sleeksky-dev/translate#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.5.1",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.3.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
